<h2>Please Input New Week</h2>

<%= form_with model: @weeks do |form| %>

    <%= form.label :title %>
    <%= form.text_field :title %>
    <br />
    <br />
    <%= form.label :content %>
    <%= form.text_field :content %>
    <br />
    <br />
    <%= form.submit "Add Next Week" %>
  
  <% end %>


  <p>
    <%= link_to  'Back Home', weeks_path %> 
</p>